{
  "name": "@agentic15.com/agentic15-claude-zen",
  "version": "8.2.0",
  "description": "Structured AI-assisted development framework for Claude Code with dual-platform support (GitHub and Azure DevOps)",
  "main": "src/index.js",
  "bin": {
    "agentic15-claude-zen": "./bin/create-agentic15-claude-zen.js",
    "agentic15": "./bin/agentic15.js"
  },
  "type": "module",
  "scripts": {
    "test": "node test/integration.test.js",
    "test:e2e": "node test/e2e-verification.test.js",
    "test:site": "node test/verify-test-site.js",
    "test:plugin": "node --test plugin/tests/**/*.test.js",
    "test:all": "npm run test && npm run test:plugin",
    "sync-readme": "node -e \"import('fs').then(fs => fs.copyFileSync('../README.md', 'README.md'))\"",
    "prepublishOnly": "npm run sync-readme"
  },
  "keywords": [
    "agentic15",
    "claude-code",
    "claude-plugin",
    "claude-zen",
    "ai-assisted-development",
    "project-scaffold",
    "workflow-enforcement",
    "workflow-automation",
    "task-tracking",
    "development-framework",
    "quality-automation",
    "git-hooks",
    "test-driven-development",
    "github-integration",
    "azure-devops",
    "dual-platform",
    "skills"
  ],
  "author": "Agentic15 <developers@agentic15.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/agentic15/claude-zen",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentic15/claude-zen.git"
  },
  "bugs": {
    "url": "https://github.com/agentic15/claude-zen/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "claudePlugin": {
    "namespace": "agentic15",
    "skills": [
      "plan",
      "task-next",
      "task-start",
      "commit",
      "sync",
      "status",
      "visual-test"
    ]
  },
  "files": [
    "bin/",
    "src/",
    "framework/",
    "templates/",
    "templates/.gitignore",
    "plugin/",
    ".claude-plugin/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "esbuild": "^0.20.0",
    "playwright": "^1.57.0"
  }
}
